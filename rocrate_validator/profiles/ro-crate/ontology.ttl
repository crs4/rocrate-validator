# # #################################################################
# # #    General axioms
# # #################################################################

[ rdf:type owl:Class ;
  owl:unionOf ( schema:Dataset
                schema:MediaObject
                rocrate:ROCrateDescriptor
              ) ;
  rdfs:subClassOf schema:CreativeWork
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( schema:Dataset
                schema:MediaObject
                rocrate:ROCrateDescriptor
              )
] .
